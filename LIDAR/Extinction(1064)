        "plt.figure(figsize=(4,4))\n",
        "\n",
        "plt.xlabel('Relative Change %')\n",
        "plt.ylabel('Hight [m]')\n",
        "\n",
        "E120 = df['LR120']*120\n",
        "E100 = df['LR100']*100\n",
        "E80 = df['LR80']*80\n",
        "E60 = df['LR60']*60\n",
